  <rdf:Description rdf:about="tag:no-measure-for-culture,2009-04-22">
    <rdf:type rdf:resource="http://purl.org/ontology/bibo/Article"/>
    <_6:startingPage rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">75</_6:startingPage>
