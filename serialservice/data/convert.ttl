  <rdf:Description rdf:about="tag:sociaaldemocraten-en-populisme,2009-04-25">
    <rdf:type rdf:resource="http://purl.org/ontology/bibo/Article"/>
    <_6:startingPage rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">15</_6:startingPage>
